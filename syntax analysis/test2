var size = n;

var arr[size];

var i = 0;
var j=  1;
var temp;

while ( i <= n ) {

        j = i + 1;
        
        while ( j <= n ){
        
                if ( arr[i] > arr[j] ) {

                        temp = arr[j];
                        arr[i] = arr[j];
                        arr[j] = temp;

                }
            j = j + 1;
        }
        i = i + 1;
} 
